CREATE TABLE av_pollutants (
          Station VARCHAR(20), DateAm VARCHAR(40), TimeAm VARCHAR(12), Ammonia DECIMAL(7,2), DateBe VARCHAR(40), TimeBe VARCHAR(12),Benzene  DECIMAL(7,2),DateCa VARCHAR(40), TimeCa VARCHAR(12), CarbonMonoxide  DECIMAL(7,2), DateND VARCHAR(40), TimeND VARCHAR(12),NitrogenDioxide  DECIMAL(7,2), DateNO VARCHAR(40), TimeNO VARCHAR(12),NitrogenOxide DECIMAL(7,2), DateON VARCHAR(40), TimeON VARCHAR(12),OxidesofNitrogen  DECIMAL(7,2), DatepX VARCHAR(40), TimepX VARCHAR(12),pXylene  DECIMAL(7,2), DateSD VARCHAR(40), TimeSD VARCHAR(12),SulphurDioxide DECIMAL(7,2), DateTo VARCHAR(40), TimeTo VARCHAR(12),Toluene DECIMAL(7,2))
        

CREATE TABLE av_meterology (
         Station VARCHAR(20),  DateAT VARCHAR(40), TimeAT VARCHAR(12), AmbientTemperature DECIMAL(7,2),  DateBP VARCHAR(40), TimeBP VARCHAR(12),BarometricPressure  DECIMAL(7,2),  DatePM10 VARCHAR(40), TimePM10 VARCHAR(12),ParticulateMatterless10  DECIMAL(7,2),  DatePM2 VARCHAR(40), TimePM2 VARCHAR(12),ParticulateMatterless2  DECIMAL(7,2), DateRH VARCHAR(40), TimeRH VARCHAR(12), RelativeHumidity DECIMAL(7,2),  DateSR VARCHAR(40), TimeSR VARCHAR(12),SolarRadiation DECIMAL(7,2),  DateVW VARCHAR(40), TimeVW VARCHAR(12),VerticalWindSpeed   DECIMAL(7,2),  DateHW VARCHAR(40), TimeHW VARCHAR(12),HorizontalWindSpeed DECIMAL(7,2), DateWD VARCHAR(40), TimeWD VARCHAR(12), WindDirection DECIMAL(7,2))
